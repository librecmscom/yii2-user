{
  "name": "yuncms/yii2-user",
  "description": "Flexible user registration and authentication module for Yii",
  "keywords": [
    "yii2",
    "yii2-user",
    "user management"
  ],
  "type": "yii2-extension",
  "license": "MIT",
  "minimum-stability": "dev",
  "require": {
    "yiisoft/yii2": "~2.0.6",
    "yiisoft/yii2-swiftmailer": "~2.0.0",
    "yiisoft/yii2-bootstrap": "~2.0.0",
    "yiisoft/yii2-imagine": "~2.1.0",
    "xutl/yii2-cropper-asset": "~1.0",
    "xutl/yii2-authclient": "~1.0",
    "xutl/yii2-fontawesome-asset": "~2.0",
    "xutl/yii2-bootstrap-filestyle-asset": "~1.0",
    "yuncms/yii2-system": "~2.1.0",
    "yuncms/yii2-tag": "~2.0.0",
    "yuncms/yii2-oauth2": "~2.0.0"
  },
  "require-dev": {
    "yiisoft/yii2-codeception": "^2.0.0",
    "codeception/specify": "^0.4.3",
    "codeception/verify": "^0.3.1"
  },
  "autoload": {
    "psr-4": {
      "yuncms\\user\\": ""
    }
  },
  "suggest": {
    "yuncms/yii2-vote": "Allows you to use vote",
    "yuncms/yii2-tag": "Allows you to use tag",
    "yuncms/yii2-message": "Allows you to use message",
    "yuncms/yii2-wallet": "Allows you to use message",
    "yuncms/yii2-im": "Allows you to use message"
  },
  "extra": {
    "bootstrap": "yuncms\\user\\Bootstrap"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },
    {
      "type": "composer",
      "url": "https://packagist.phpcomposer.com"
    }
  ]
}